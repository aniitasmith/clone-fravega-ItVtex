{
  "search-result-layout.mobile#department": {
    "children": [
      "search-title.v2",
      "flex-layout.row#department-breadcrumb-mobile",
      "flex-layout.row#department-category-results-mobile"
    ],
    "props": {
      "showFacetQuantity": true,
      "defaultGalleryLayout": "gallery",
      "blockClass": "container-search"
    }
  },
  "flex-layout.row#department-breadcrumb-mobile": {
    "children": [
      "flex-layout.col#department-breadcrumb-mobile"
    ],
    "props": {
      "blockClass": "container-row"
    }
  },
  "flex-layout.col#department-breadcrumb-mobile": {
    "children": [
      "breadcrumb.search"
    ],
    "props": {
      "blockClass": "breadcrumb"
    }
  },
  "flex-layout.row#department-category-results-mobile": {
    "children": [
      "flex-layout.col#category-results-filter-department-mobile",
      "flex-layout.col#category-results-content-department-mobile"
    ],
    "props": {
      "blockClass": "container-category-results"
    }
  },
  "flex-layout.col#category-results-filter-department-mobile": {
    "children": [
      "filter-navigator.v3"
    ],
    "props": {
      "blockClass": "container-category-filter"
    }
  },
  "flex-layout.col#category-results-content-department-mobile": {
    "children": [
      "search-content#results-content-products-department-mobile"
    ],
    "props": {
      "blockClass": "container-results-content-products-department-mobile"
    }
  },
  "search-content#results-content-products-department-mobile": {
    "blocks": [
      "gallery#department",
      "not-found"
    ]
  }
}
