{
  "header-row#header__middle":{
    "title":"Header Middle",
    "children":[
      "header-row#header__middle--logo",
      "header-spacer",
      "header-row#header__middle--search",
      "header-spacer",
      "header-row#header__middle--account"
    ],
    "props":{
      "blockClass":"header__middle",
      "fullWidth": true
    }
  },
  "header-row#header__middle--logo": {
    "children": [
      "logo#header__middle--logo",
      "header-row#logo-location"
    ],
    "props": {
      "blockClass": "header__middle-row-logo"
    }
  },
  "logo#header__middle--logo":{
    "title": "logo",
    "props": {
      "url":"assets/img/fravega-logo.svg",
      "width": 150,
      "href": "/"
    }
  },
  "header-row#logo-location": {
    "children": [
      "image#location",
      "rich-text#location"
    ],
    "props": {
      "blockClass": "header__middle-row-location"
    }
  },
  "image#location": {
    "props": {
      "blockClass": "Location",
      "src": "assets/img/icons-location.svg",
      "maxHeight": ""
    }
  },
  "rich-text#location": {
    "props": {
      "text": "Est√°s en Capital Federal",
      "blockClass": "text-location"
    }
  },
  "header-row#header__middle--search": {
    "children": [
      "search-bar"
    ],
    "props": {
      "blockClass": "header__middle-row-search"
    }
  },
  "search-bar": {
    "blocks": ["autocomplete-result-list.v2"],
    "props": {
      "openAutocompleteOnFocus": true
    }
  },
  "autocomplete-result-list.v2": {
    "blocks": ["product-summary"]
  },
  "header-row#header__middle--account": {
    "children": [
      "login",
      "minicart.v2#desktop"
    ],
    "props": {
      "blockClass": "header__middle-row-account"
    }
  },
  "login": {
    "props": {
      "emailAndPasswordTitle": "Login",
      "accessCodeTitle": "Acess Code LOG-IN",
      "emailPlaceholder": "e-mail",
      "passwordPlaceholder": "contrasena",
      "showPasswordVerificationIntoTooltip": true,
      "showIconProfile": true,
      "iconLabel":" Mi cuenta"
    }
  },
  "minicart.v2#desktop": {
    "props": {
      "MinicartIcon": "icon#home__account--fravega",
      "variation": "popup",
      "itemCountMode": "total",
      "quantityDisplay": "always"
    },
    "children": [
      "minicart-base-content"
    ]
  },
  "icon#home__account--fravega": {
    "props": {
      "id": "hpa-shopping",
      "size": 16,
      "viewBox":"0 0 25 25"
    }
  },
  "minicart-base-content":{
    "blocks":[
      "minicart-empty-state"
    ],
    "children":[
      "minicart-product-list",
      "flex-layout.row#minicart-footer"
    ],
    "props":{
      "blockClass":"minicart-base-content"
    }
  },
  "flex-layout.row#minicart-footer": {
    "props": {
      "blockClass": "minicart-footer"
    },
    "children": ["flex-layout.col#minicart-footer"]
  },
  "flex-layout.col#minicart-footer": {
    "children": ["minicart-checkout-button"]
  },
  "minicart-product-list": {
    "blocks": ["product-list#minicart"]
  },
  "product-list#minicart": {
    "blocks": ["product-list-content-mobile"]
  },
  "minicart-checkout-button": {
    "props": {
      "finishShoppingButtonLink": "/checkout/#/orderform"
  }
  },
  "minicart-empty-state": {
    "children": ["flex-layout.row#empty-state"],
    "props":{
      "blockClass":"minicart-empty-state"
    }
  },
  "flex-layout.row#empty-state": {
    "children": [
      "flex-layout.col#img-empty-state",
      "flex-layout.col#text-empty-state"
    ],
    "props": {
      "blockClass": "content-row-empty-state",
      "preventHorizontalStretch": true
    }
  },
  "flex-layout.col#img-empty-state": {
    "children": [
      "image#minicart-empty-state"
    ],
    "props": {
      "blockClass": "content-img-empty-state",
      "verticalAlign": "middle"
    }
  },
  "image#minicart-empty-state": {
    "props": {
      "src": "assets/img/carrito.svg",
      "blockClass": "icon-minicart-empty-state" 
    }
  },
  "flex-layout.col#text-empty-state": {
    "children": [
      "rich-text#minicart-line-1-empty-state",
      "rich-text#minicart-line-2-empty-state"
    ],
    "props": {
      "blockClass":"content-col-empty-state"
    }
  },
  "rich-text#minicart-line-1-empty-state": {
    "props": {
      "text": "Tu carrito esta vacio",
      "blockClass": "minicart-line-1",
      "textAlignment": "LEFT"
    }
  },
  "rich-text#minicart-line-2-empty-state": {
    "props": {
      "text": "Descubri las categorias del sitio y elegi los mejores productos.",
      "blockClass": "minicart-line-2",
      "textAlignment": "LEFT"
    }
  }
}