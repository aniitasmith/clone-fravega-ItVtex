{
  "store.search#department": {
    "blocks": [
      "search-result-layout#department"
    ],
    "props": {
      "context": {
        "skusFilter": "FIRST_AVAILABLE",
        "simulationBehavior": "default"
      }
    }
  },
  "search-result-layout#department": {
    "blocks": [
      "search-result-layout.desktop#department",
      "search-result-layout.mobile#department",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop#department": {
    "children": [
      "flex-layout.row##department-breadcrumb",
      "flex-layout.row#category-results"
    ]
  },
   "flex-layout.row##department-breadcrumb": {
    "children": [
      "flex-layout.col#department-breadcrumb"
    ]
  },
  "flex-layout.col#department-breadcrumb": {
    "children": [
      "breadcrumb.search"
    ]
 },
   "flex-layout.row#category-results": {
    "children": [
      "flex-layout.col#category-results-filter",
      "flex-layout.col#category-results-content"
    ]
  },
  "flex-layout.col#category-results-filter": {
    "children": [
      "filter-navigator.v3"
    ]
  },
  "flex-layout.col#category-results-content": {
    "children": [
      "search-content#results-content-products"
    ]
  },
  "search-content#results-content-products": {
    "blocks": [
      "gallery#department",
      // "gallery-layout-switcher",
      "not-found"
    ]
  },
