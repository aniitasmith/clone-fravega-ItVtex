  //   }
  // },




  "gallery#department": {
    "props": {
      "layouts": [
        {
          "name": "general-grid",
          "component": "GeneralGrid",
          "itemsPerRow": {
            "desktop": 4,
            "tablet": 4,
            "phone": 4
          }
        }
      ],
      "GeneralGrid": "product-summary.shelf"
    }
  }, 
  "search-result-layout.mobile#department": {
    "children": [
      "flex-layout.row#category-results"
    ]
  }
}
