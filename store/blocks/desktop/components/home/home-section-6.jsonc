{
  "flex-layout.row#home__section-6": {
    "children": [
      "flex-layout.col#home-container-section-6"
    ],
    "props": {
      "blockClass": "container-section-6"
    }
  },
  "flex-layout.col#home-container-section-6": {
    "title": "List Context Ofertas",
    "children": [
      "rich-text#home__section-6",
      "list-context.product-list#section-6"
    ],
    "props": {
      "blockClass": "home-container-section-6"
    }
  },
  "rich-text#home__section-6": {
    "title": "Text ofertas destacadas",
    "props": {
      "text": "**Aprovech√° las ofertas destacadas**",
      "blockClass": "text-section-6"
    }
  },
  "list-context.product-list#section-6": {
    "blocks": [
      "product-summary.shelf#home__section-6"
    ],
    "children": [
      "slider-layout#products-section-6"
    ],
    "props": {
      "blockClass": "list-context-section-6"
    }
  },
  "product-summary.shelf#home__section-6": {
    "children": [
      "stack-layout#summary-image",
      "product-summary-name",
      "product-summary-price"
    ],
    "props": {
      "blockClass": "product-summary-shelf-section-6"
    }
  },
  "slider-layout#products-section-6": {
    "title": "Slider ofertas",
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 4,
        "phone": 2
      },
      "infinite": false,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "slider-products-section-6",
      "showPaginationDots": "never"
    }
  }
}
